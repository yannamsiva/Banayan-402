
public class ArrayEx3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//Jagged Array
		int ar[][]=new int[3][];
		
		ar[0]=new int[3];
		ar[1]=new int[2];
		ar[2]=new int[1];
		
		for(int i=0;i<ar.length;++i)
		{
			for(int j=0;j<ar[0].length;++j)
			{
				System.out.print(ar[j]);
			}
			System.out.println();
		}
		
		
		
		
	}

}
